  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "@philpl/buble": "^0.19.7",
    "@prisma/client": "^2.0.0-beta.2",
    "@reach/router": "^1.3.3",
    "algoliasearch": "^4.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "downshift": "^5.0.5",
    "gatsby": "^2.20.28",
    "gatsby-image": "^2.0.33",
    "gatsby-plugin-algolia": "^0.5.0",
    "gatsby-plugin-catch-links": "^2.3.0",
    "gatsby-plugin-google-analytics": "^2.2.4",
    "gatsby-plugin-google-tagmanager": "^2.9.0",
    "gatsby-plugin-layout": "^1.3.4",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-mdx": "^1.2.34",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-remove-trailing-slashes": "^2.2.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-plugin-smoothscroll": "^1.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.2.5",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-source-remote-file": "^0.1.9",
    "gatsby-transformer-remark": "^2.3.2",
    "gatsby-transformer-sharp": "^2.1.17",
    "is-absolute-url": "^3.0.3",
    "lodash": "^4.17.15",
    "polished": "^3.5.1",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "qs": "^6.11.0",
    "react": "^16.8.4",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.8.4",
    "react-feather": "^2.0.9",
    "react-ga": "^3.0.0",
    "react-helmet": "^6.0.0",
    "react-hooks-global-state": "^1.0.0",
    "react-hooks-testing-library": "^0.3.6",
    "react-instantsearch-dom": "^6.33.0",
    "react-live": "^2.2.2",
    "react-loadable": "^5.5.0",
    "react-sticky": "^6.0.3",
    "react-testing-library": "^6.0.0",
    "smooth-scroll": "^16.1.2",
    "styled-components": "^4.1.3",
    "styled-components-ts": "^0.0.15",
    "write": "^2.0.0"
  },
  "keywords": [
    "dataguide"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "build": "./scripts/build",
    "dev": "gatsby develop",
    "serve": "npm run build && gatsby serve --prefix-paths -o",
    "prettify": "prettier --write \"**/*.{js,mdx,json,md,ts,tsx,yml,toml,graphql,css}\"",
    "install-algolia": "cd ./plugins/gatsby-algolia-indexer && npm install",
    "test": "jest"
  },
  "devDependencies": {
    "@mdx-js/loader": "1.6.22",
    "@mdx-js/tag": "0.20.3",
    "@types/jest": "^24.0.11",
    "@types/qs": "^6.9.7",
    "@types/react-helmet": "^5.0.8",
    "@types/react-instantsearch-dom": "^6.12.3",
    "@types/styled-components": "^4.1.12",
    "gatsby-remark-sectionize": "^1.0.0",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "loader-utils": "^1.2.3",
    "prettier": "^1.16.4",
    "prisma": "^4.6.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
